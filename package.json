    "@vue/test-utils": "^1.2.2",
    "babel-core": "^6.26.3",
    "babel-jest": "^27.3.1",
    "eslint": "^8.1.0",
    "jest": "^27.3.1",
    "vue-jest": "^3.0.7"
  }
}
